]0;tom976@cscie-2x-i: ~/26-hw6[01;32mtom976@cscie-2x-i[00m:[01;34m~/26-hw6[00m$ nl -b a myrm
     1	#!/bin/sh
     2	#
     3	# myrm - Has file removing/restoring capability like windows recycle bin
     4	# usage - Accepts options -t, -l, -L, -p, -a, -g as arugments to be explained below
     5	# The logger statements also dictate the intent of the code
     6	
     7	    main()
     8	    {
     9	    
    10	        option="$0"
    11	
    12	        while [ $# -gt 0 ]
    13	        do
    14	           
    15	            if is_option "$1"
    16	            then
    17	                option="$1"
    18	                process_file "" $option # Process empty file if no option
    19	            else 
    20	                process_file "$1" $option
    21	            fi 
    22	
    23		    shift 
    24	
    25	        done
    26	
    27	    }
    28	
    29	    # Driver function
    30	    process_file()
    31	    {
    32	        option="$2"
    33	        file="$1"
    34	       
    35	        case $option in
    36	               *t)  process_t "$file" $option ;;
    37	               *l)  process_l "$file" $option ;;
    38	               *L)  process_L "$file" $option ;;
    39	               *p)  process_p "$file" $option ;;
    40	               *a)  process_a "$file" $option ;;
    41	               *g)  process_g "$file" $option ;;
    42	               *myrm) process_no_option "$file" $option ;;
    43	               *)   echo "usage [myrm | -t | -l | -L | -p | -a | -g]"
    44	                    exit 1 ;;
    45	        esac 
    46	
    47	    }
    48	
    49	   # Moves files to thrash room
    50	    process_no_option()
    51	    {
    52	       file="$1"
    53	
    54	       if [ ! -d $HOME/thrashroom ]
    55	       then
    56	           echo Made directory $HOME/thrashroom
    57	           mkdir $HOME/thrashroom
    58	       fi
    59	       
    60	       if [ ! -f $file ] && [ ! -d $file ]
    61	       then
    62	          echo The file you are trying to remove does not exist
    63	          return 1
    64	       fi
    65	
    66	       if [ -f $HOME/thrashroom/$file ] || [ -d $HOME/thrashroom/$file ]
    67	       then
    68	          echo there is already a file called $file in the thrash y to replace it?
    69	          read ans
    70	
    71	          case $ans in
    72	              y*) mv $file $HOME/thrashroom 
    73	                  echo Moved $file to thrashroom
    74	                  return 0 ;;
    75	              *)  echo Not overwriting file 
    76	                  return 1;;
    77	          esac
    78	       else
    79	           echo Moved $file to thrashroom
    80	           mv $file $HOME/thrashroom
    81	       fi
    82	    }
    83	
    84	    # Removes files completely
    85	    process_t()
    86	    {
    87	        num_files=$(ls -la $HOME/thrashroom | wc -l) 
    88		echo OK to delete these $num_files items y/n?
    89	        read ans
    90	
    91	        case $ans in
    92	           y*)  ls -A1 $HOME/thrashroom/* | xargs rm -r
    93	                echo Permanently deleted $num_files of files from thrashroom
    94	                return 0 ;;
    95	           n*) echo Not deleting file 
    96	                return 1 ;;
    97	           *)  echo usage [Yy|Nn] 
    98	                return 1 ;;
    99	        esac  
   100	    }
   101	
   102	    # Lists what is in the thrash room
   103	    process_l()
   104	    {
   105	        echo Files in thrash room below
   106	        ls -A $HOME/thrashroom
   107	
   108	    }
   109	
   110	    process_L()
   111	    {
   112	        echo Long list of files in the thrash room
   113	        ls -lA $HOME/thrashroom
   114	    }
   115	
   116	    # Recovers or throws out all the files in the thrash room
   117	    process_p()
   118	    {
   119	        files=$(ls -A1 $HOME/thrashroom/*)
   120	
   121	        for file in $files
   122	        do
   123	            echo file $file throw out or recover?
   124	            read ans
   125	            
   126	            if [ $? = 0 ]
   127	            then
   128	              case $ans in
   129		        t*) rm $file 
   130			    echo Threw out file $file ;;
   131	                r*) 
   132	                   if [ -f "$file" ] || [ -d "$file" ]
   133	                   then 
   134	                       echo there is already a file named $file in your directory y to replace it?
   135	                       read replac
   136	
   137	                       if [ $? = 0 ]
   138	                       then 
   139	                            case $replac in
   140	                               y*) echo Recovered $file file from thrashroom
   141	                                   mv $file . ;;
   142	                               *) Not removing file ;;
   143	                            esac
   144	                       fi
   145	                   else 
   146	                       echo Recovered $file file from thrashroom
   147	                       mv $file .
   148	                   fi ;;
   149	                *) Not moving file 
   150	                   return 1 ;;
   151	             esac
   152	           fi 
   153	        done
   154	    }
   155	
   156	    # Placeholder to show that this has been handled
   157	    process_a()
   158	    {
   159	        echo All of the other commands have been processed with a flag already
   160	    }
   161	
   162	    # Moves what is in the thrash room to current directory
   163	    process_g()
   164	    {
   165	         
   166	        file="$1"
   167	
   168	        if [ -z $file ]
   169	        then 
   170	           return 1 
   171	        fi
   172	 
   173	        if [ ! -f $HOME/thrashroom/$file ] && [ ! -d $HOME/thrashroom/$file ]
   174	        then
   175	           echo File or Directory does not exist
   176	           return 1
   177	        else 
   178	            echo Moved $file file from thrashroom to current directory
   179	            mv $HOME/thrashroom/$file .
   180	            return 0
   181	        fi 
   182	    }
   183	
   184	    # Helper function to distinguish file vs argument
   185	    is_option()
   186	    {
   187	       OCCURENCE=$(echo "$1" | tr -cd '-' | wc -c)
   188	       
   189	       if [ $OCCURENCE -gt 0 ]
   190	       then 
   191	          case "$1" in
   192	             *[tlLpga]) return 0 ;;
   193	             *) return 1 ;;
   194	          esac
   195	       else 
   196	           return 1
   197	       fi 
   198	    }
   199	    
   200	    main "$@"
